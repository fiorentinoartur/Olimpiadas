             x:Class="App1.WsTowerFlyout"
            IconImageSource="baseline_info_24"
             xmlns:pages="clr-namespace:App1">
    
  <FlyoutPage.Flyout>
    <pages:WsTowerFlyoutFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:WsTowerFlyoutDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>